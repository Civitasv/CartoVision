<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.carto.vo.Student" table="student" schema="cartovision">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="dept" column="dept"/>
        <property name="studyNum" column="study_num"/>
        <property name="qq" column="qq"/>
        <property name="introduction" column="introduction"/>
    </class>
</hibernate-mapping>